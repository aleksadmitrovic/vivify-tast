{
  "name": "react-movie-cards",
  "version": "1.0.0",
  "description": "A basic application that displays a list of movies as a list of cards",
  "scripts": {
    "build": "webpack -d",
    "build:watch": "webpack --watch",
    "serve": "webpack -p && live-server ./public",
    "serve:dev": "webpack-dev-server --open",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drminnaar/react-movie-cards.git"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "react",
      "es2016"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "live-server": "^1.2.0",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
